    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Reddrake",
            "email": "eagleftf123@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Waimao\\AmazonMws\\": "src"
        }
    },
    "minimum-stability": "stable"
}
